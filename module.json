{
	"id": "permission-manager",
	"title": "Permission Manager",
	"description": "A module that provides a way for custom permissions similar to Foundry's default permission system.",
	"version": "1.0.0",
	"compatibility": {
		"minimum": 12,
		"verified": 12,
		"maximum": 12
	},
	"authors": [
		{
			"name": "Oliver Akins",
			"url": "https://oliver.akins.me"
		}
	],
	"esmodules": [
		"src/main.mjs"
	],
	"styles": [
		"styles/_index.css"
	],
	"languages": [
		{
			"lang": "en",
			"name": "English (Canadian)",
			"path": "langs/en-ca.json"
		}
	],
	"url": "",
	"manifest": "",
	"download": "",
	"readme": "README.md",
	"bugs": "",
	"library": true,
	"flags": {
		"hotReload": {
			"extensions": ["css", "hbs", "json", "js", "mjs", "svg"],
			"paths": ["templates", "langs", "styles", "src", "assets"]
		},
		"permission-manager": {
			"customPermissions": [
				{
					"id": "canEditPermissions",
					"gm": "always",
					"name": "PM.Permission.canEditPermissions.name",
					"hint": "PM.Permission.canEditPermissions.hint",
					"default": [4]
				}
			]
		}
	}
}