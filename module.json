{
	"id": "permission-manager",
	"title": "Custom Permission Lib",
	"description": "A module that provides a way for custom permissions similar to Foundry's default permission system.",
	"version": "1.0.1",
	"compatibility": {
		"minimum": 12,
		"verified": 12,
		"maximum": 12
	},
	"authors": [
		{
			"name": "Oliver Akins",
			"url": "https://oliver.akins.me"
		}
	],
	"esmodules": [
		"src/main.mjs"
	],
	"styles": [
		"styles/_index.css"
	],
	"languages": [
		{
			"lang": "en",
			"name": "English (Canadian)",
			"path": "langs/en-ca.json"
		}
	],
	"url": "https://github.com/Oliver-Akins/Foundry-Permission-Manager",
	"manifest": "https://github.com/Oliver-Akins/Foundry-Permission-Manager/releases/latest/download/module.json",
	"download": "https://github.com/Oliver-Akins/Foundry-Permission-Manager/releases/latest/download/release.zip",
	"readme": "README.md",
	"bugs": "https://github.com/Oliver-Akins/Foundry-Permission-Manager/issues/new?labels=Bug%2CTriage&template=1-bug.yaml&title=%5BBug%5D%3A+",
	"library": true,
	"flags": {
		"hotReload": {
			"extensions": ["css", "hbs", "json", "js", "mjs", "svg"],
			"paths": ["templates", "langs", "styles", "src", "assets"]
		},
		"permission-manager": {
			"customPermissions": []
		}
	}
}